<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_facerecognition">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.cekmitl.pdpacameracensor.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.cekmitl.pdpacameracensor.ui.gallery.GalleryFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_gallery" />


    <fragment
        android:id="@+id/navigation_facerecognition"
        android:name="com.cekmitl.pdpacameracensor.ui.facerecognition.FaceRecognitionFragment"
        android:label="Face Recognition"
        tools:layout="@layout/fragment_face_recognition" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.cekmitl.pdpacameracensor.ui.setting.SettingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting" >
    </fragment>

</navigation>